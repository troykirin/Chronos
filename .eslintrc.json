{
  "env": {
    "browser": true,
    "es6": true,
    "react-native/react-native": true
  },
  "extends": ["plugin:react/recommended", "google"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 11,
    "sourceType": "module"
  },
  "plugins": ["react", "react-native"],
  "rules": {}
}
